  <h4 id="home"><%= link_to "home", user_path(current_user) %></h4>
  <h4><%= link_to "edit profile", edit_user_path(current_user) %></h4>
  <h4><%= link_to "my resources", user_resources_path(current_user) %></h4>

  <% if current_user_is_coach? %>
    <h4><%= link_to "add resource", new_user_resource_path(current_user) %></h4>
    <%if current_user_is_coach? && current_user.student %>
      <h4><%= link_to "my student", user_student_path(current_user.student) %></h4>
    <% end %>

  <% elsif current_user_is_student? && current_user.coach_id.nil? %>
    <h4><%= link_to "find a coach", find_a_coach_path %></h4>
  <% elsif current_user_is_student? && current_user.coach_id %>
    <h4><%= link_to "my coach", user_coach_path(current_user.coach) %></h4>
  <% end %>

  <h4><%= link_to "top resources", top_resources_path %></h4>
  <h4><%= link_to "logout", logout_path %></h4>
